<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane minHeight="600.0" minWidth="1200.0" prefHeight="600.0" prefWidth="1200.0" stylesheets="@../css/Main.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.todoapp.controller.MainController">
   <children>
      <BorderPane minHeight="600.0" minWidth="1200.0" prefHeight="600.0" prefWidth="1200.0" AnchorPane.leftAnchor="0.0">
         <left>
            <VBox id="left-bar" prefHeight="562.0" prefWidth="234.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox id="top-left-bar" fx:id="hBox" styleClass="top-bar">
                     <children>
                        <Button id="new-list-button" fx:id="newListButtton" mnemonicParsing="false" onAction="#onHandleClick" text="Nova Lista" />
                        <Text id="title-app" strokeType="OUTSIDE" strokeWidth="0.0" text="To do" />
                     </children>
                  </HBox>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
